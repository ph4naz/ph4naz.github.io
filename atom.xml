<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>paperplanes. software development that flies</title>
 <link href="http://ph4naz.github.io/atom.xml" rel="self"/>
 <link href="http://ph4naz.github.io"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://ph4naz.github.io</id>
 
 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://ph4naz.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://ph4naz.github.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>